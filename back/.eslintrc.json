{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi":"warn",
        "quotes":"off",
        "linebreak-style":"off",
        "no-unused-vars":"off",
        "no-console":"off",
        "max-len":"off",
        "no-useless-escape":"off",
        "consistent-return":"off"
    }
}
